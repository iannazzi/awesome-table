    "es6Watch": "webpack --config ./webpack/webpack.es6Demo.config.js --mode development --watch",
    "globalDemo": "webpack --config ./webpack/webpack.globalDemo.config.js --mode development",
    "globalDemoWatch": "webpack --config ./webpack/webpack.globalDemo.config.js --mode development --watch",
    "dist": "webpack --config ./webpack/webpack.config.js --mode development",
    "watch": "webpack --config ./webpack/webpack.config.js --mode development --watch",
    "mocha": "mocha test/puppeteerBootstrap.js --recursive test",
    "server": "http-server -p 8081",
    "karma": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "jest": "jest",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*"
  },
  "repository": {
    "type": "git",
